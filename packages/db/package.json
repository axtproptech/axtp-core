{
  "name": "@axtp/db",
  "version": "1.0.11",
  "description": "The database client package",
  "author": "ohager",
  "license": "MIT",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "push-db": "prisma db push --skip-generate",
    "request-deploy:prod": "pscale dr create axt-core stage --deploy-to main",
    "deploy": "pscale dr deploy axt-core",
    "generate-db": "prisma generate",
    "prebuild": "npm run generate-db",
    "build": "tsup",
    "predev": "npm run generate-db",
    "dev": "tsup --dev"
  },
  "prisma": {},
  "devDependencies": {
    "@types/node": "^16.11.47",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1"
  }
}
