    "build": "tsup",
    "predev": "npm run generate",
    "dev": "tsup --dev"
  },
  "prisma": {},
  "devDependencies": {
    "@types/node": "^16.11.47",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1"
  }
}
