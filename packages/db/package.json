{
  "name": "@axt/db-package",
  "version": "1.0.6",
  "description": "The database client package",
  "author": "ohager",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "push": "prisma db push",
    "request-deploy:stage": "pscale dr create rest-core stage",
